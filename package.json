{
  "name": "frozenshield",
  "version": "1.0.0",
  "description": "Modern fullstack portfolio website for showcasing custom web development services. Features MongoDB backend, contact form, and comprehensive SEO optimization for Canada's northern territories.",
  "main": "server/server.js",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "start": "node server/server.js",
    "dev": "nodemon server/server.js",
    "seed": "node server/seedTestData.js",
    "build": "echo 'No build step required for Node.js backend'",
    "test": "cross-env NODE_ENV=test jest --runInBand",
    "test:watch": "cross-env NODE_ENV=test jest --watch --runInBand",
    "test:coverage": "cross-env NODE_ENV=test jest --coverage --runInBand",
    "test:models": "cross-env NODE_ENV=test jest tests/models --runInBand",
    "test:api": "cross-env NODE_ENV=test jest tests/api --runInBand",
    "test:integration": "cross-env NODE_ENV=test jest tests/integration --runInBand",
    "lint": "echo 'Run: npm install -D eslint && npx eslint .'",
    "security-check": "npm audit",
    "update-deps": "npm outdated && npm update",
    "docker:build": "docker build -t frozenshield:latest .",
    "docker:run": "docker run -p 5000:5000 --env-file .env frozenshield:latest",
    "docker:compose": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f"
  },
  "keywords": [
    "portfolio",
    "web-development",
    "fullstack",
    "nodejs",
    "express",
    "mongodb",
    "mongoose",
    "contact-form",
    "seo-optimization",
    "yellowknife",
    "northwest-territories",
    "yukon",
    "nunavut",
    "canada",
    "web-design",
    "responsive-design",
    "rest-api",
    "rate-limiting"
  ],
  "author": "FrozenShield Studio <hello@frozenshield.ca>",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^3.0.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.3.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.3",
    "mongodb": "^6.20.0",
    "mongoose": "^8.0.3",
    "morgan": "^1.10.0",
    "multer": "^2.0.2",
    "nodemailer": "^7.0.12",
    "sharp": "^0.34.5"
  },
  "devDependencies": {
    "@shelf/jest-mongodb": "^6.0.2",
    "cross-env": "^10.1.0",
    "jest": "^30.2.0",
    "nodemon": "^3.0.2",
    "supertest": "^7.1.4"
  }
}
